<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Comment</key>
	<string>Created with Recipe Robot v2.5.0 (https://github.com/homebysix/recipe-robot)</string>
	<key>Description</key>
	<string>Downloads the latest version of Fanny and creates a package.</string>
	<key>Identifier</key>
	<string>com.github.emetriq.autopkg.pkg.Fanny</string>

	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>Fanny</string>
	</dict>

	<key>MinimumVersion</key>
	<string>2.3</string>

	<key>ParentRecipe</key>
	<string>com.github.emetriq.autopkg.download.Fanny</string>

	<key>Process</key>
	<array>

		<!-- ensure %found_filename% is available in this recipe too -->
		<dict>
			<key>Processor</key>
			<string>FileFinder</string>
			<key>Arguments</key>
			<dict>
				<key>pattern</key>
				<string>%RECIPE_CACHE_DIR%/%NAME%/**/%NAME%.app</string>
			</dict>
		</dict>

		<dict>
			<key>Processor</key>
			<string>AppPkgCreator</string>
			<key>Arguments</key>
			<dict>
				<key>app_path</key>
				<string>%found_filename%</string>
				<key>pkg_path</key>
				<string>%RECIPE_CACHE_DIR%/%NAME%-%version%.pkg</string>
			</dict>
		</dict>

	</array>
</dict>
</plist>